//User function Template for c 
//Function to rotate an array by d element in counter-clockwise direction

void reverse(int arr[], int start, int end) {
        while(start<end){
        int temp = int start;
        int start = int end;
        int end = temp;
        start++;
        end--;
        }
      }
 void rotateArr(int arr[], int d, int n){
       //code here
       d = d%n;
       reverse(arr,0,d-1);
       reverse(arr,d,n-1);
       reverse(arr,0,n-1);
       
   }
       
     
